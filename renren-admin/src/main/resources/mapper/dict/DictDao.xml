<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="DictDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.sys.entity.Demo01Entity" id="demo01Map">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
    </resultMap>

    <select id="selectByValue" resultType="io.renren.common.entity.PageData">
        SELECT id FROM sys_dict WHERE `value` = #{value}
    </select>

</mapper>