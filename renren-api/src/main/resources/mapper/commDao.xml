<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="commDao">

    <select id="selectComm" resultType="io.renren.common.entity.PageData">
        SELECT * FROM
        sys_dict
        WHERE 1=1
        <if test="typeId != null">
            AND dictgroupid = #{typeId}
        </if>
        <if test="parentValue != null">
            AND parentid = #{parentValue}
        </if>
    </select>
</mapper>